{
  "app_name": "PraPazar Enterprise (Dev)",
  "app_version": "1.0.0",
  "app_description": "Enterprise E-commerce Integration Platform",
  "environment": {
    "_value_": "development",
    "_name_": "DEVELOPMENT",
    "__objclass__": "<enum 'Environment'>",
    "_sort_order_": 0
  },
  "debug": true,
  "timezone": "Europe/Istanbul",
  "locale": "tr_TR",
  "url": "http://localhost:5000",
  "database": {
    "driver": "mysql",
    "host": "localhost",
    "port": 3306,
    "database": "prapazar_dev",
    "username": "root",
    "password": "",
    "charset": "utf8mb4",
    "collation": "utf8mb4_unicode_ci",
    "pool_size": 5,
    "pool_timeout": 30,
    "pool_recycle": 3600,
    "ssl_mode": "PREFERRED",
    "ssl_ca": null,
    "ssl_cert": null,
    "ssl_key": null,
    "backup_enabled": true,
    "backup_interval": 86400,
    "backup_retention": 7
  },
  "redis": {
    "host": "localhost",
    "port": 6379,
    "password": null,
    "database": 0,
    "ssl": false,
    "ssl_cert_reqs": "required",
    "ssl_ca_certs": null,
    "ssl_certfile": null,
    "ssl_keyfile": null,
    "max_connections": 100,
    "retry_on_timeout": true,
    "health_check_interval": 30
  },
  "security": {
    "secret_key": "change-this-super-secret-key-in-production",
    "jwt_secret": "change-this-jwt-secret-key-in-production",
    "jwt_expiration": 3600,
    "jwt_refresh_expiration": 604800,
    "password_min_length": 8,
    "password_require_uppercase": true,
    "password_require_lowercase": true,
    "password_require_numbers": true,
    "password_require_symbols": true,
    "max_login_attempts": 5,
    "login_lockout_duration": 900,
    "session_timeout": 3600,
    "csrf_protection": true,
    "cors_origins": [
      "*"
    ],
    "rate_limit_enabled": true,
    "rate_limit_requests": 1000,
    "rate_limit_window": 3600,
    "encryption_key": null
  },
  "logging": {
    "level": "DEBUG",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file_enabled": true,
    "file_path": "logs/enterprise.log",
    "file_max_size": 10485760,
    "file_backup_count": 5,
    "console_enabled": true,
    "syslog_enabled": false,
    "syslog_host": "localhost",
    "syslog_port": 514,
    "json_format": false,
    "include_caller": true,
    "audit_enabled": true,
    "audit_file": "logs/audit.log"
  },
  "monitoring": {
    "enabled": false,
    "metrics_enabled": true,
    "health_check_enabled": true,
    "health_check_interval": 60,
    "prometheus_enabled": false,
    "prometheus_port": 9090,
    "grafana_enabled": false,
    "grafana_url": "http://localhost:3000",
    "alerting_enabled": true,
    "alert_email": "admin@prapazar.com",
    "alert_webhook": null,
    "uptime_threshold": 99.9,
    "response_time_threshold": 2000
  },
  "integration": {
    "enabled": true,
    "timeout": 30,
    "retry_count": 3,
    "retry_delay": 1.0,
    "circuit_breaker_enabled": true,
    "circuit_breaker_threshold": 5,
    "circuit_breaker_timeout": 300,
    "rate_limiting_enabled": true,
    "caching_enabled": true,
    "cache_ttl": 300,
    "webhook_enabled": true,
    "webhook_timeout": 10,
    "batch_processing": true,
    "batch_size": 100,
    "parallel_processing": true,
    "max_workers": 10
  },
  "email": {
    "driver": "smtp",
    "host": "smtp.gmail.com",
    "port": 587,
    "username": "",
    "password": "",
    "encryption": "tls",
    "from_address": "noreply@prapazar.com",
    "from_name": "PraPazar Enterprise",
    "timeout": 30,
    "max_retries": 3,
    "templates_path": "templates/emails",
    "queue_enabled": true,
    "queue_driver": "redis"
  },
  "storage": {
    "driver": "local",
    "local_path": "storage",
    "public_path": "public/uploads",
    "url_prefix": "/uploads",
    "max_file_size": 10485760,
    "allowed_extensions": [
      ".jpg",
      ".jpeg",
      ".png",
      ".gif",
      ".pdf",
      ".doc",
      ".docx",
      ".xls",
      ".xlsx"
    ],
    "aws_enabled": false,
    "aws_bucket": "",
    "aws_region": "us-east-1",
    "aws_access_key": "",
    "aws_secret_key": "",
    "cdn_enabled": false,
    "cdn_url": ""
  },
  "custom_settings": {}
}